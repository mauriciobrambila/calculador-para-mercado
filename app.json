        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "enable16kPageSupport": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "34ae94c3-7638-41b5-a3f0-776fb8882f96"
