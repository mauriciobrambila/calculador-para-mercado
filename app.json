    "slug": "aplicativoMercado",
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "android": {
      "package": "com.mauricio2025.appmercado",
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
